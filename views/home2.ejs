<div class="center">
    <table class="table table-striped table-dark">
        <thead>
          <tr>
            <th scope="col">team</th>
            <th scope="col">captian name</th>
            <th scope="col">1st position</th>
            <th scope="col">2nd position</th>
            <th scope="col">3rd position</th>
            <th scope="col">kills</th>
            <th scope="col">points</th>
          </tr>
        </thead>
        <tbody>
            <% for (let index = 0; index < array.length; index++) { %>
                <tr>
                    <td> <%= array[index].team %></td>
                    <td> <%= array[index].p1.name %></td>
                    <td> <%= array[index].pos1 %></td>
                    <td> <%= array[index].pos2 %></td>
                    <td> <%= array[index].pos3 %></td>
                    <td> <%= array[index].kills %></td>
                    <td> <%= array[index].points %></td>
                </tr>
           <% } %>
        </tbody>
      </table>
</div>

